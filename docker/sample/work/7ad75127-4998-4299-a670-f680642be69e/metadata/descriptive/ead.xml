<?xml version="1.0" encoding="UTF-8"?>
<ead xmlns="http://ead3.archivists.org/schema/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ead3.archivists.org/schema/ ../../schemas/ead3.xsd">
	<!--A required child element of <ead> for recording bibliographic and administrative information about an EAD instance.-->
	<control>
	<!--Record Identifier. A required child element of <control> that designates a unique identifier for the EAD instance.-->
		<recordid>7ad75127-4998-4299-a670-f680642be69e</recordid>
		<filedesc>
			<!--Title Statement. A required child element of <filedesc> that binds together information about the name of an encoded finding aid and those responsible for its content.-->
			<titlestmt>
				<titleproper>Example of geographic data</titleproper>
			</titlestmt>
			<!--Note Statement. A child element of notestmt, used to provide a general note related to the EAD instance.-->
			<notestmt>
				<controlnote>
					<p>Note Statement</p>
				</controlnote>
			</notestmt>
		</filedesc>
		<!--A required child element of <control> that records the current version status of the EAD instance.-->
		<maintenancestatus value="new">text</maintenancestatus>
		<maintenanceagency>
			<agencyname>text</agencyname>
		</maintenanceagency>
		<maintenancehistory>
			<maintenanceevent>
				<eventtype value="created">text</eventtype>
				<eventdatetime>text</eventdatetime>
				<agenttype value="human">text</agenttype>
				<agent>text</agent>
			</maintenanceevent>
		</maintenancehistory>
	</control>

	<!--Archival Description. A required child element of <ead> that binds together all of the archival descriptive information in an EAD instance.-->
	<archdesc level="collection">
		<did>
			<abstract>text</abstract>
		</did>
		<dsc dsctype="combined">
			<!-- An aggregation level - series -->
			<c01 level="series" >
				<did>
					<unitid>Series 1</unitid>
					<unittitle>Correspondence</unittitle>
				</did>
				<!-- If the file plan is related to an aggregation (series) -->
				<fileplan>
					<head>File List</head>
					<fileplan>
						<head>DILCIS</head>
						<list listtype="ordered" numeration="upper-roman">
							<item>Marketing</item>
						</list>
					</fileplan>
				</fileplan>
				<!-- An aggregation level - sub-series -->
				<c02 level="subseries">
					<did>
						<unitid>Subseries</unitid>
						<unittitle>Subseries</unittitle>
					</did>
					<c03 level="file">
						<did>
							<unittitle>Example of geographic data</unittitle>
						</did>
						<!-- An aggregation level - item -->
						<c04 level="item">
							<did>
								<!--Universally unique identifier for an entity that is generated automatically by the system and stays with the entity forever-->
								<unitid localtype="system_id">7ad75127-4998-4299-a670-f680642be69e</unitid>
								<!--The identifying name or title of the entity-->
								<unittitle>Example of geographic data</unittitle>
								<!--System set date and time when the entity was created. Can be represented as unitdatestructured as well.-->
								<unitdate datechar="created">17.03.2020</unitdate>
								<!--Date and time of origin of a record or other entity which may vary from the creation date of the entity in the system. Can be represented as unitdate as well.-->
								<unitdatestructured datechar="originated">
									<datesingle>17.03.2020</datesingle>
								</unitdatestructured>
								<abstract>Description of the entity</abstract>
								<!--The unit used to describe the extent of the record (e.g MB, pages, num of files/components)-->
								<physdescstructured coverage="whole" physdescstructuredtype="spaceoccupied">
									<quantity>1</quantity>
									<unittype>page</unittype>
								</physdescstructured>
								
									<daoset label="gml" coverage="whole">
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/b6eb897d-a886-4700-9654-ad1513c12c61/representations/b6eb897d-a886-4700-9654-ad1513c12c61/data/D46_GML.gml" localtype="fullsize"
												href="../../representations/b6eb897d-a886-4700-9654-ad1513c12c61/data/D46_GML.gml"/>
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/b6eb897d-a886-4700-9654-ad1513c12c61/representations/b6eb897d-a886-4700-9654-ad1513c12c61/data/D46_ATR.csv" localtype="fullsize"
												href="../../representations/b6eb897d-a886-4700-9654-ad1513c12c61/data/D46_ATR.csv"/>
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/b6eb897d-a886-4700-9654-ad1513c12c61/representations/b6eb897d-a886-4700-9654-ad1513c12c61/data/D46_GML.xsd" localtype="fullsize"
												href="../../representations/b6eb897d-a886-4700-9654-ad1513c12c61/data/D46_GML.xsd"/>
										
									</daoset>
								
									<daoset label="shp" coverage="whole">
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/ded90639-6617-4d89-a269-4391e5121428/representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.prj" localtype="fullsize"
												href="../../representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.prj"/>
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/ded90639-6617-4d89-a269-4391e5121428/representations/ded90639-6617-4d89-a269-4391e5121428/data/D46_ATR.dbf" localtype="fullsize"
												href="../../representations/ded90639-6617-4d89-a269-4391e5121428/data/D46_ATR.dbf"/>
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/ded90639-6617-4d89-a269-4391e5121428/representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.dbf" localtype="fullsize"
												href="../../representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.dbf"/>
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/ded90639-6617-4d89-a269-4391e5121428/representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.shx" localtype="fullsize"
												href="../../representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.shx"/>
										
										<dao daotype="borndigital"
												linktitle="info:uuid/7ad75127-4998-4299-a670-f680642be69e/ded90639-6617-4d89-a269-4391e5121428/representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.shp" localtype="fullsize"
												href="../../representations/ded90639-6617-4d89-a269-4391e5121428/data/D46.shp"/>
										
									</daoset>
								
							</did>
							<!--Related Material-->
							<relatedmaterial>
								<p>For identifying archival materials that have an association to the materials being described.</p>
							</relatedmaterial>
							<!--Other Descriptive Data-->
							<odd localtype="OwnElementDescription">
								<p>For recording additional information about the described materials that is not easily incorporated into one of the other named elements within archdesc and c.</p>
							</odd>
							<!-- If the file plan is related to an item (a record). The classification of the entity, used by child aggregations and records. -->
							<!-- Contains an ID as an attribute.-->
							<fileplan id="d8d4d161-e48e-4ec4-84df-73606f6c35e1">
								<head>File List</head>
								<fileplan>
									<head>DILCIS</head>
									<list listtype="ordered" numeration="upper-roman">
										<item>Marketing</item>
									</list>
								</fileplan>
							</fileplan>
						</c04>
					</c03>
				</c02>
			</c01>
		</dsc>
	</archdesc>
</ead>